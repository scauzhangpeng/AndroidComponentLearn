<manifest package="com.wlteck.maincomponent"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.wlteck.maincomponent.debug.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_news"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_news"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".mvp.view.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
